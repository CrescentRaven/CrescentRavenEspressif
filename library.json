{
    "name": "CrescentRaven Espressif",
    "keywords": "CrescentRaven,esp8266,esp32,espressif",
    "description": "CrescentRaven Espressif common utilities library.",
    "repository": {
        "type": "git",
        "url": "https://github.com/CrescentRaven/CrescentRavenEspressif"
    },
    "version": "1.0.0",
    "license": "MIT",
    "exclude": "tests",
    "frameworks": "arduino",
    "platforms": [
        "espressif8266",
        "espressif32"
    ],
    "authors": {
        "name": "Travis J. Gutjahr",
        "email": "support@crescentraven.com",
        "url": "https://www.crescentraven.com",
        "maintainer": true
    },
    "dependencies": [
        {
            "name": "CrescentRaven/CrescentRaven",
            "version": "https://github.com/CrescentRaven/CrescentRaven#1.0.0"
        }
    ],
    "headers": "CrescentRavenEspressif.h",
    "build": {
        "unflags": "-std=gnu99 -std=gnu++11",
        "flags": "-std=gnu17 -std=gnu++2a"
    }
}